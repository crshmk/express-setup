{
  "name": "express-setup",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -r dotenv/config --exec babel-node src/index.js",
    "test": "jest --watchAll --no-cache --detectOpenHandles" 
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ramda": "^0.29.1"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/*.test.js"
    ],
    "globals": {
      "DB_HOST": "localhost"
    },
    "moduleNameMapper": {
      "~(.*)$": "<rootDir>/src/$1",
      "^app(.*)$": "<rootDir>/src/app$1",
      "^auth(.*)$": "<rootDir>/src/auth$1",
      "^routes(.*)$": "<rootDir>/src/routes$1"
    }
  }
}
